        "import torch\n",
        "import torch.nn as nn\n",
        "from tqdm import tqdm\n",
        "import numpy as np\n",
        "\n",
        "from datasets import load_dataset\n",
        "from transformers import GPT2Tokenizer, GPT2LMHeadModel, TextDataset, DataCollatorForLanguageModeling\n",
        "from transformers import DistilBertTokenizer, DistilBertForSequenceClassification\n",
        "from transformers import BertForSequenceClassification\n",
        "\n",
        "from transformers import Trainer, TrainingArguments\n",
        "from transformers import AutoModelForCausalLM, AutoTokenizer\n",
        "\n",
        "import torch.optim as optim\n",
        "from torch.optim.lr_scheduler import ReduceLROnPlateau\n",
        "\n",
        "from transformers import SchedulerType"
      ],
      "metadata": {
        "id": "4TD_tlY2QYbh"
      },
      "execution_count": 4,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "source": [
        "# Emotion-Aware Chatbot"
      ],
      "metadata": {
        "id": "p_IfWKidcffd"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Load the model and tokenizer\n",
        "model = GPT2LMHeadModel.from_pretrained('microsoft/DialoGPT-medium')\n",
        "tokenizer = AutoTokenizer.from_pretrained('microsoft/DialoGPT-medium', padding_side='left')\n",
        "tokenizer.pad_token = tokenizer.eos_token\n",
        "\n",
        "# Load the dataset\n",
        "dataset_path = \"/content/drive/MyDrive/combined_dataset.json\"\n",
        "dataset = load_dataset(\"json\", data_files=dataset_path)\n",
        "\n",
        "# Split the dataset into training and validation sets\n",
        "split_dataset = dataset[\"train\"].train_test_split(test_size=0.1)  # 90% train, 10% validation\n",
        "train_dataset = split_dataset[\"train\"]\n",
        "validation_dataset = split_dataset[\"test\"]\n",
        "\n",
        "# Define emotion dictionary (if the dataset has emotion labels)\n",
        "emotion_dict = {0: 'neutral', 1: 'anger', 2: 'disgust', 3: 'fear', 4: 'happiness', 5: 'sadness', 6: 'surprise'}\n",
        "\n",
        "# Preprocessing function for the new dataset\n",
        "def encode_with_emotion(examples):\n",
        "    dialog_with_emotions = []\n",
        "    for context, response in zip(examples[\"Context\"], examples[\"Response\"]):  # Use \"Context\" and \"Response\"\n",
        "        # Combine context and response into a single string\n",
        "        dialog = f\"Context: {context} Response: {response}\"\n",
        "        dialog_with_emotions.append(dialog)\n",
        "\n",
        "    # Tokenize the conversations\n",
        "    encoded = tokenizer(dialog_with_emotions, truncation=True, padding='max_length', max_length=128)\n",
        "    encoded['labels'] = encoded['input_ids'][:]  # Use input_ids as labels for language modeling\n",
        "\n",
        "    return encoded\n",
        "\n",
        "# Apply preprocessing\n",
        "train_dataset = train_dataset.map(encode_with_emotion, batched=True)\n",
        "validation_dataset = validation_dataset.map(encode_with_emotion, batched=True)\n",
        "\n",
        "# Data collator for language modeling\n",
        "data_collator = DataCollatorForLanguageModeling(tokenizer=tokenizer, mlm=False)\n",
        "\n",
        "# Training arguments\n",
        "from transformers import SchedulerType\n",
        "\n",
        "training_args = TrainingArguments(\n",
        "    output_dir=\"./DialoGPT-medium-chatbot-emotion-aware\",  # Updated output directory\n",
        "    overwrite_output_dir=True,\n",
        "    num_train_epochs=10,\n",
        "    per_device_train_batch_size=8,  # Reduced batch size for DialoGPT-medium\n",
        "    per_device_eval_batch_size=32,  # Reduced batch size for DialoGPT-medium\n",
        "    warmup_steps=0,\n",
        "    weight_decay=0.01,\n",
        "    learning_rate=3e-5,  # Reduced learning rate for DialoGPT-medium\n",
        "    lr_scheduler_type=SchedulerType.COSINE,\n",
        "    save_total_limit=2,\n",
        "    prediction_loss_only=True,\n",
        "    fp16=True,\n",
        "    save_strategy=\"steps\",\n",
        "    save_steps=4000,\n",
        "    eval_strategy=\"steps\",  # Updated to avoid deprecation warning\n",
        "    eval_steps=500,\n",
        "    load_best_model_at_end=True,\n",
        "    metric_for_best_model=\"loss\",\n",
        "    seed=42\n",
        ")\n",
        "\n",
        "# Initialize the Trainer\n",
        "trainer = Trainer(\n",
        "    model=model,\n",
        "    args=training_args,\n",
        "    data_collator=data_collator,\n",
        "    train_dataset=train_dataset,  # Use the training split\n",
        "    eval_dataset=validation_dataset  # Use the validation split\n",
        ")\n",
        "\n",
        "# Fine-tune the model\n",
        "trainer.train()\n",
        "\n",
        "# Save the fine-tuned model and tokenizer\n",
        "trainer.save_model(\"./DialoGPT-medium-chatbot-emotion-aware\")\n",
        "tokenizer.save_pretrained(\"./DialoGPT-medium-chatbot-emotion-aware\")\n",
        "\n",
        "# Load the fine-tuned model and tokenizer for testing\n",
        "tokenizer = AutoTokenizer.from_pretrained(\"./DialoGPT-medium-chatbot-emotion-aware\", padding_side='left')\n",
        "model = AutoModelForCausalLM.from_pretrained(\"./DialoGPT-medium-chatbot-emotion-aware\")"
      ],
      "metadata": {
